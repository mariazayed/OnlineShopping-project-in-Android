<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.project.onlineshopping">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OnlineShopping">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"></activity>
        <activity android:name=".Buy"></activity>
        <activity android:name=".Admin"></activity>
        <activity android:name=".Owners"></activity>
        <activity android:name=".Add_Owner"></activity>
        <activity android:name=".Show_Owners"></activity>
        <activity android:name=".Delete_Owner"></activity>
        <activity android:name=".Show_Products"></activity>
    </application>
</manifest>